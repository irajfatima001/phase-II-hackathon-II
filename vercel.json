{
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/.next",
  "installCommand": "cd frontend && npm install",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://irajfatima-phase2-backend.hf.space/api/:path*"
    },
    {
      "source": "/auth/:path*",
      "destination": "https://irajfatima-phase2-backend.hf.space/auth/:path*"
    }
  ]
}